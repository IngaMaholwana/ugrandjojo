{
  "name": "ugrandjo",
  "displayName": "ugrandjo",
  "description": "VS Code extension with Hypedawg",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:hypedawg.hypedawgView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ugrandjo.hello",
        "title": "Hello World"
      },
      {
        "command": "ugrandjo.hypedawg",
        "title": "Hype me!"
      },
      {
        "command": "ugrandjo.changeBreed",
        "title": "Change Hypedawg Breed"
      }
    ],
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "hypedawg.hypedawgView",
          "name": "Hypedawg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "ugrandjo.hypedawg",
          "when": "view == hypedawg.hypedawgView",
          "group": "navigation"
        },
        {
          "command": "ugrandjo.changeBreed",
          "when": "view == hypedawg.hypedawgView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/node": "16.x",
    "typescript": "^4.8.4",
    "eslint": "^8.26.0"
  }
}